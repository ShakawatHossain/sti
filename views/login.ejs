<div class="wrapper fadeInDown">
  <div id="formContent">
    <!-- Tabs Titles -->

    <!-- Icon -->
    <div class="fadeIn first">
      <img src="https://d1dt4ip8cptu11.cloudfront.net/assets/images/logos/logo-name-a91cad3e9c87649a5d4c8dd127c5ce77.png" id="icon" alt="User Icon" />
    </div>

    <!-- Login Form -->
    <form action="" method="post">
      <input type="text" id="login" class="fadeIn second" name="user_id" placeholder="login">
      <input type="password" id="password" class="fadeIn third" name="pass" placeholder="password">
      <p class="warning_cls"><%=typeof result!='undefined'?result:'';%></p>
      <input type="submit" class="fadeIn fourth" value="Log In">
    </form>

    <!-- Remind Passowrd -->
    <div id="formFooter">
      <!-- <a class="underlineHover" href="#">Forgot Password?</a> -->
    </div>

  </div>
</div>